#read skin.tin
#read ../spells.tin
#read ../food.tin
#read ../path/path.tin

#variable {CHAR} {NIL}
#action {By what name do you wish to be known?}
{
    #variable {CHAR} {Raelen};
    $CHAR;
}

#alias {get-bag}
{
    get-recs;
    mid-knights;
    get bag;
    get all bag;
    wear all;
}

#alias {hide-bag}
{
    get-recs;
    mid-knights;
    #13 {rem skin;put skin bag};
    drop bag;
    camo bag;
}

#event {PROGRAM START}
{
    #session jedimud jedimud.net 4000
}
